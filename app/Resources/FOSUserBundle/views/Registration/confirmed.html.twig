


<!DOCTYPE html>
<html lang="fr">
    
  <head>
      
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>{% trans %}site.titre{% endtrans %}</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100italic,100,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    
    {% stylesheets filter='cssrewrite'
    '@ExiaCoreBundle/Resources/public/css/fix.css'
    '@ExiaCoreBundle/Resources/public/css/theme.bootstrap.css' 
    %}
    
    <link rel="stylesheet" href="{{ asset_url }}" >
    {% endstylesheets %}
    
  </head>
  
  <body>
    
        <div class="well" id="bloc_connexion">
        	<div id="logo_connexion">
                    {% image '@ExiaCoreBundle/Resources/public/img/test.png' output="images/test.png" %}
                        <img src="{{ asset_url }}" width="100%" height="100%" alt="Logo Greta">
                    {% endimage %}
                </div>


			{% block fos_user_content %}
    			<p>{{ 'registration.confirmed'|trans({'%username%': user.username}, 'FOSUserBundle') }}</p>
    			{% if app.session is not empty %}
        			{% set targetUrl = app.session.get('_security.' ~ app.security.token.providerKey ~ '.target_path') %}
    			{% endif %}
			{% endblock fos_user_content %}

			<div>
				<a href="{{ path('exia_core_homepage') }}">Accueil</a>
			</div>
        </div>
        
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <script src="http://d3js.org/d3.v3.min.js"></script>
    
    {% javascripts %}
    <script src="{{ asset_url }}"></script>
            
    {% endjavascripts %}
    
    </body>
    
</html>

